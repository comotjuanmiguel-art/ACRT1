print("STRING ACTIVITY")
print('-'*40)
name = input("Please enter your full name: ")
print("first name: " + name[0:1])
print("last name: " + name[16:100])
print("5 characters of the name: " + name[0:5])
print("reserve name: " + name[::-1])
print("every character of the name: " + name[::2])
print("middle name: " + name[5:12])
